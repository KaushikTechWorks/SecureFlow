{
  "ServiceName": "secureflow-app",
  "SourceConfiguration": {
    "RepoType": "GITHUB",
    "GitHubConfiguration": {
      "RepositoryUrl": "https://github.com/KaushikTechWorks/SecureFlow.git",
      "BranchName": "main",
      "ConfigurationSource": "REPOSITORY"
    },
    "AutoDeploymentsEnabled": true
  },
  "InstanceConfiguration": {
    "Cpu": "0.25 vCPU",
    "Memory": "0.5 GB"
  },
  "HealthCheckConfiguration": {
    "Protocol": "HTTP",
    "Path": "/",
    "Interval": 30,
    "Timeout": 5,
    "HealthyThreshold": 2,
    "UnhealthyThreshold": 5
  }
}
